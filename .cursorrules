import requests
import json
from concurrent.futures import ThreadPoolExecutor, as_completed
from itertools import islice

def batch_fetch(api_base, start, end, batch_size=100):
    """æ‰¹é‡è·å–APIæ•°æ®"""
    session = requests.Session()
    results = []
    
    def fetch_one(id):
        try:
            r = session.get(f"{api_base}/{id}", timeout=5)
            if r.status_code == 200:
                return r.json()
        except:
            pass
        return None
    
    with ThreadPoolExecutor(max_workers=100) as executor:
        futures = {executor.submit(fetch_one, i): i for i in range(start, end)}
        for future in as_completed(futures):
            result = future.result()
            if result:
                results.append(result)
    
    return results

