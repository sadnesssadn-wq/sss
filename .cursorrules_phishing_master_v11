    $element = $_GET['el'] ?? 'unknown';
    
    $db = new SQLite3($tracking_db);
    $stmt = $db->prepare('INSERT INTO clicks (tracking_token, element) VALUES (?, ?)');
    $stmt->bindValue(1, $token);
    $stmt->bindValue(2, $element);
    $stmt->execute();
    
    $stmt = $db->prepare('UPDATE visits SET clicked=1 WHERE tracking_token=?');
    $stmt->bindValue(1, $token);
    $stmt->execute();
    
