def forge_business_card(target_info):
    """
    ä¼ªé€ åç‰‡
    
    ç”¨é€”: ä¾›åº”é“¾é’“é±¼ï¼Œä¼ªè£…æˆä¾›åº”å•†å‘˜å·¥
    """
    from PIL import Image, ImageDraw, ImageFont
    
    # åˆ›å»ºåç‰‡
    img = Image.new('RGB', (600, 350), color='white')
    draw = ImageDraw.Draw(img)
    
    # æ·»åŠ å…¬å¸logoï¼ˆå…‹éš†çœŸå®logoï¼‰
    # draw.text(...)
    
    # æ·»åŠ å‘˜å·¥ä¿¡æ¯
    draw.text((50, 100), f"å§“å: {target_info['name']}", fill='black')
    draw.text((50, 150), f"èŒä½: {target_info['title']}", fill='black')
    draw.text((50, 200), f"é‚®ç®±: {target_info['fake_email']}", fill='black')
    draw.text((50, 250), f"ç”µè¯: {target_info['fake_phone']}", fill='black')
    
    img.save('forged_business_card.png')
    print("[âœ…] ä¼ªé€ åç‰‡å·²ç”Ÿæˆ")

def forge_email_screenshot(sender, subject, body):
    """
    ä¼ªé€ é‚®ä»¶æˆªå›¾
    
    ç”¨é€”: åœ¨ç¤¾äº¤å·¥ç¨‹ä¸­"å±•ç¤º"çœ‹ä¼¼å¯ä¿¡çš„é‚®ä»¶
    """
    # ä½¿ç”¨Outlook/Gmailç•Œé¢æ¨¡æ¿ç”Ÿæˆé€¼çœŸæˆªå›¾
    template = """
    <!DOCTYPE html>
    <html>
    <head>
        <style>
            /* å®Œç¾æ¨¡ä»¿Outlookç•Œé¢ */
        </style>
    </head>
    <body>
        <div class="outlook-interface">
            <div class="header">å‘ä»¶äºº: {sender}</div>
            <div class="subject">ä¸»é¢˜: {subject}</div>
            <div class="body">{body}</div>
        </div>
    </body>
    </html>
    """
    
    # æ¸²æŸ“å¹¶æˆªå›¾
    # ä½¿ç”¨Selenium + Headless Chrome
    from selenium import webdriver
    
    options = webdriver.ChromeOptions()
    options.add_argument('--headless')
    driver = webdriver.Chrome(options=options)
    
    # åŠ è½½HTMLå¹¶æˆªå›¾
    driver.get('data:text/html,' + template.format(sender=sender, subject=subject, body=body))
    driver.save_screenshot('fake_email.png')
    
    print("[âœ…] é‚®ä»¶æˆªå›¾å·²ç”Ÿæˆ")

