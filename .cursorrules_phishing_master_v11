### 1.6 GitHubæƒ…æŠ¥æŒ–æ˜

```bash
# ä»GitHubæŒ–æ˜æ•æ„Ÿä¿¡æ¯
cat > osint/github/scrape.sh << 'EOFSH'
#!/bin/bash

ORG_NAME="example-corp"

echo "â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”"
echo "ğŸ™ GitHubæƒ…æŠ¥æŒ–æ˜"
echo "â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”"

# å‘˜å·¥è¯†åˆ«
echo "[*] è¯†åˆ«ç»„ç»‡æˆå‘˜"
gh api orgs/$ORG_NAME/members | jq -r '.[].login' > members.txt
echo "[âœ…] å‘ç° $(wc -l < members.txt) ä¸ªæˆå‘˜"

# æœç´¢æ•æ„Ÿä¿¡æ¯
echo "[*] æœç´¢å¯†é’¥æ³„éœ²"
gh search code "password OR secret OR api_key OR token" --owner $ORG_NAME > secrets_search.txt

# æå–é‚®ç®±
echo "[*] æå–æäº¤è€…é‚®ç®±"
for repo in $(gh repo list $ORG_NAME --json name -q '.[].name'); do
    gh api repos/$ORG_NAME/$repo/commits | \
        jq -r '.[].commit.author.email' >> commit_emails.txt
done
cat commit_emails.txt | sort -u > unique_emails.txt
echo "[âœ…] æå– $(wc -l < unique_emails.txt) ä¸ªé‚®ç®±"

# è¯†åˆ«å¸¸ç”¨æŠ€æœ¯æ ˆ
echo "[*] è¯†åˆ«æŠ€æœ¯æ ˆ"
for repo in $(gh repo list $ORG_NAME --json name -q '.[].name' | head -10); do
    langs=$(gh api repos/$ORG_NAME/$repo/languages | jq -r 'keys[]')
    echo "$repo: $langs"
done > tech_languages.txt

echo "[âœ…] GitHubæƒ…æŠ¥æ”¶é›†å®Œæˆ"
EOFSH

chmod +x osint/github/scrape.sh
# éœ€è¦å…ˆ gh auth login
# ./osint/github/scrape.sh
```

