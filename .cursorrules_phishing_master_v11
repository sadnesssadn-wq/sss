```python
# é‚®ä»¶ä¸ªæ€§åŒ–ç”Ÿæˆå™¨
cat > templates/personalize_email.py << 'PYEOF'
#!/usr/bin/env python3
import json
from jinja2 import Template
from datetime import datetime, timedelta

def personalize_email(template_file, target, scenario):
    """
    æ ¹æ®ç›®æ ‡ä¿¡æ¯ä¸ªæ€§åŒ–é‚®ä»¶å†…å®¹
    """
    with open(template_file, 'r', encoding='utf-8') as f:
        template = Template(f.read())
    
    # ç”Ÿæˆè¿½è¸ªtoken
    import hashlib
    tracking_token = hashlib.md5(f"{target['email']}{datetime.now()}".encode()).hexdigest()[:16]
    
    # è®¡ç®—æˆªæ­¢æ—¶é—´
    deadline = (datetime.now() + timedelta(days=2)).strftime("%Yå¹´%mæœˆ%dæ—¥ %H:00")
    
    # ä¸ªæ€§åŒ–å˜é‡
    context = {
        'TARGET_NAME': target['name'],
        'EMPLOYEE_ID': target.get('id', '000000'),
        'PHISHING_DOMAIN': 'vpn-portal-secure.com',  # é’“é±¼åŸŸå
        'TRACKING_TOKEN': tracking_token,
        'DEADLINE': deadline,
        'GITHUB_USERNAME': target.get('github', 'user'),
        'ORG_NAME': 'example-corp',
        'REPO_NAME': 'production-app',
        'CURRENT_YEAR': datetime.now().year,
    }
    
    # åŸºäºå¿ƒç†ç”»åƒè°ƒæ•´å†…å®¹
    if target.get('psychology'):
        psych = target['psychology']
        
        # å¦‚æœç›®æ ‡æŠ±æ€¨VPNæ…¢ï¼Œå¼ºè°ƒ"æ€§èƒ½æå‡"
        if 'VPN' in str(psych.get('pain_points', [])):
            context['UPGRADE_BENEFIT'] = 'âš¡ é€Ÿåº¦æå‡300%'
        
        # å¦‚æœç›®æ ‡å…³æ³¨å®‰å…¨ï¼Œå¼ºè°ƒ"å®‰å…¨å‡çº§"
        if 'å®‰å…¨' in target.get('title', ''):
            context['SECURITY_NOTE'] = 'ğŸ”’ ç¬¦åˆæœ€æ–°åˆè§„è¦æ±‚'
    
    # æ¸²æŸ“æ¨¡æ¿
    personalized_html = template.render(**context)
    
    # ç”Ÿæˆå¯¹åº”çš„çº¯æ–‡æœ¬ç‰ˆæœ¬
    from html2text import html2text
    personalized_text = html2text(personalized_html)
    
    return {
        'html': personalized_html,
        'text': personalized_text,
        'tracking_token': tracking_token,
        'variables': context
    }

# ç¤ºä¾‹ä½¿ç”¨
target = {
    "name": "å¼ ä¸‰",
    "email": "zhangsan@example.com",
    "id": 1001,
    "title": "CEO",
    "github": "zhangsan",
    "psychology": {
        "pain_points": ["å…¬å¸VPNå¤ªæ…¢"]
    }
